{
  "name": "netflex/customers",
  "description": "Netflex Customers library",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Olav Ã˜yen",
      "email": "olav@apility.no"
    },
    {
      "name": "Thomas Alrek",
