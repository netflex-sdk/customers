    "netflex/support": "^2.0.0-RC1"
  },
  "require-dev": {
    "illuminate/container": "^6.16",
    "illuminate/support": "^6.16",
    "larapack/dd": "^1.1",
    "vlucas/phpdotenv": "^3.6"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Netflex\\Customers\\Providers\\AuthServiceProvider"
      ],
      "aliases": {
        "Customer": "Netflex\\Customers\\Customer"
      }
    }
  }
}
